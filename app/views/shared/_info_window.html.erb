<div class="container">
  <div>
    <p class="pop-title"><%= house.name %></p>
    <p class="pop-time"><%= t('house.created_at') %> <%= house.created_at.strftime("%d-%m-%Y") %></p>
    <p class="pop-user"><%= house.address %></p>
    <%= link_to path do %>
      <i class="fas fa-eye"></i>
    <% end %>
  </div>
</div>
